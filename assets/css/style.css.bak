.boxPop {
  position: fixed;
  right: 16px;
  bottom: 16px;
  border-radius: 2px;
  border: solid 1px #e3e3e3;
  box-shadow: -1px 0px 6px 0 rgba(0,0,0,0.3);
  background-color: #151515;
  background: linear-gradient(to bottom, #151515 0%, #343434 100%);
  padding: 30px;
  max-width: 200px;
/*  transition: all 0.1s linear 0s;
  opacity: 1;*/
/*  animation-name: example;
  animation-duration: 0.2s;*/
}

.boxPop .txt-1 {
  display: block;
  font-size: 15px;
  font-weight: 500;
  color: #fff;
  margin: 0px 0px 16px 0;
  padding: 0 0 7px 0;
  border-bottom: solid 1px #ffffff2b;
}
.boxPop .txt-2 {
  color: #fff;
  font-size: 12px;
  font-weight: 600;
  margin: 0;
  padding: 0;
/*  display: block;*/
}
.boxPop .txt-3 {
  color: #ffffff;
  font-size: 11px;
  font-weight: 300;
  margin: 5px 0px 0px 0px;
  padding: 0px;
  display: block;
}
.boxPop .excluir {
  color: #fff;
  background-color: #f4511e;
  padding: 0px 3px;
  line-height: 19px;
  margin: 0 2px 0 0px;
  border-radius: 15px;
  display: inline-block;
  transition: all 0.1s linear 0s;
}
.boxPop .excluir:hover {
  color: #fff;
  background-color: rgb(244,81,30);
}
.boxPop .ico {
  color: #fff;
  font-size: 12px;
  font-weight: 700;
  margin: 0px;
  padding: 0px;
  vertical-align: inherit;
  max-width: initial;
  width: initial;
  max-height: initial;
  height: initial;
  line-height: 0;
}
.boxPop .ico2 {
    color: #fff;
    font-size: 16px;
    line-height: 1.4;
}
.boxPop .layout-row {

}

.boxPop button {font-size: 12px; padding: 0px 20px;}

